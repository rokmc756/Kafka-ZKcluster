tickTime={{ _zookeeper.config.tick_time }}
dataDir={{ _zookeeper.config.data_dir }}
clientPort={{ _zookeeper.config.port }}
initLimit={{ _zookeeper.config.init_limit }}
syncLimit={{ _zookeeper.config.sync_limit }}
{% for server in groups["zk_servers"] %}
{% if server == inventory_hostname %}
server.{{ loop.index }}=0.0.0.0:2888:3888
{% else %}
server.{{ loop.index }}={{ server }}:2888:3888
{% endif %}
{% endfor %}
